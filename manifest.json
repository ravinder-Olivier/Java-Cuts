{
"name": "JavaCuts",
"description": "Shortcuts for all those Javascripts kids are using nowadays",
"version": "1.0",
"manifest_version": 3,
"author": "Ravinder Olivier Singh Dadiala", 
"permissions": [
    "activeTab",
    "pageCapture",
    "scripting",
    "system.display",
    "declarativeContent",
    "cookies"

],
"icons": {
    "16": "icons/16px.png",
    "48": "icons/48px.png",
    "128": "icons/128px.png"
},
"action": {
    "default_popup": "index.html",
    "default_title": "JavaCuts",
    "default_icon": {
        "16": "icons/16px.png",
        "48": "icons/48px.png",
        "128": "icons/128px.png"
    }
}
}